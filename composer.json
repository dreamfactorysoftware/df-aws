{
  "name":        "dreamfactory/df-aws",
  "description": "AWS package containing services for the DreamFactory Platform 2.0.",
  "keywords":    [
    "dreamfactory",
    "api",
    "rest",
    "aws",
    "s3",
    "dynamodb",
    "redshift"
  ],
  "homepage":    "https://www.dreamfactory.com/",
  "license":     "Apache-2.0",
  "authors":     [
    {
      "name":  "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    },
    {
      "name":  "Lee Hicks",
      "email": "leehicks@dreamfactory.com"
    }
  ],
  "support":     {
    "email":  "dspsupport@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/df-aws",
    "issues": "https://github.com/dreamfactorysoftware/df-aws/issues",
    "wiki":   "https://wiki.dreamfactory.com"
  },
  "require":     {
    "dreamfactory/df-database": "~0.9.1",
    "dreamfactory/df-email":    "~0.8.0",
    "dreamfactory/df-file":     "~0.6.0",
    "dreamfactory/df-sqldb":    "~0.15.1",
    "aws/aws-sdk-php":          "3.*"
  },
  "autoload":    {
    "psr-4": {
      "DreamFactory\\Core\\Aws\\": "src/"
    }
  },
  "extra":       {
    "branch-alias": {
      "dev-develop": "0.15.x-dev"
    },
    "laravel": {
      "providers": [
        "DreamFactory\\Core\\Aws\\ServiceProvider"
      ]
    }
  }
